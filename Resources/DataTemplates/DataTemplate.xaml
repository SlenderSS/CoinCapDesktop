<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:CoinCapDesktop.Views"
                    xmlns:vm="clr-namespace:CoinCapDesktop.ViewModels">


    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <v:HomeView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AssetsViewModel}">
        <v:AssetsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AssetDetailsViewModel}">
        <v:AssetDetailsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ExchangesViewModel}">
        <v:ExchangesView />
    </DataTemplate>

</ResourceDictionary>